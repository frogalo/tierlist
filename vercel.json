{
  "version": 2,
  "builds": [
    {
      "src": ".robo/build/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": ".robo/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/tierlist/(.*)",
      "destination": "/.robo/build/api/tierList/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/.robo/build/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/.robo/public/$1"
    }
  ]
}
