{
  "version": 2,
  "builds": [
    {
      "src": "build/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": ".robo/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/build/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/.robo/public/$1"
    },
    {
      "cleanUrls": true
    }
  ]
}
