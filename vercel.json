{
  "version": 2,
  "builds": [
    {
      "src": ".robo/build/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": ".robo/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/.robo/build/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/.robo/public/$1"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/.robo/public/$1.png"
    },
    {
      "src": "/(.*)",
      "dest": "/.robo/public/$1"
    }
  ]
}
